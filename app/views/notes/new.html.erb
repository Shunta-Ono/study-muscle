<h1>Notes#new</h1>
<%= form_for @note do |f| %>
<%= f.label :tile, 'タイトル' %>
<%= f.text_field :title %>
<%= f.label :name, '筋肉名' %>
<%= f.text_field :name %>
<%= f.label :origin, '起始' %>
<%= f.text_field :origin %>
<%= f.label :insertion, '停止' %>
<%= f.text_field :insertion %>
<%= f.label :innervation, '支配神経' %>
<%= f.text_field :innervation %>
<%= f.label :action, '作用' %>
<%= f.text_field :action %>
<%= f.label :body, '内容' %>
<%= f.text_area :body, 'data-provider': :summernote %>
<%= f.submit '作成する' %>
<% end %>